          "200": {
            "description": "successful signup",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "client": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "email": {
                          "type": "string"
                        }
                      }
                    },
                    "token": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "client": {
                    "email": "12233test@gmail.com",
                    "name": "Ivasuk"
                  },
                  "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiI2NTM3ZjVlN2YwYzIxNjA5Yzc5M2NjY2YiLCJpYXQiOjE2OTgxNjYyNDcsImV4cCI6MTY5ODIzODI0N30.mKzizoialddNQG5OSXoEfvfoXtp38fZuyvbxdwwbQVw"
