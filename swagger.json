                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              },
              "example": {
                "email": "12233test@gmail.com",
                "password": "ivasukTelesuk",
                "name": "Ivasuk"
              }
            }
          },
          "description": "`name` - string, required.`email` - string email required.`password` - string, min 6, required."
        },
