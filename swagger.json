    "/identification/calculateDailyMetrics": {
      "post": {
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "tags": ["Identification"],
        "summary": "count BMR",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "height": {
                    "type": "number"
                  },
                  "currentWeight": {
                    "type": "number"
                  },
                  "desiredWeight": {
                    "type": "number"
                  },
                  "birthday": {
                    "type": "string",
                    "format": "date",
                    "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                  },
                  "blood": {
                    "type": "number"
                  },
                  "sex": {
                    "type": "string"
                  },
                  "levelActivity": {
                    "type": "number"
                  }
                }
              },
              "example": {
                "birthday": "2003-10-10",
                "blood": 4,
                "currentWeight": 55,
                "desiredWeight": 55,
                "height": 155,
                "levelActivity": 4,
                "sex": "male"
              }
            }
          },
          "description": "`height` - number; minimum 150; required .`currentWeight` - number; minimum 35; required .`desiredWeight` - number; minimum 35; required .`birthday` - date; must be older than 18 ;  required .`blood` - number; allowed values 1, 2, 3, 4; required .`sex` - string; allowed values 'male', 'female'; required .`levelActivity` - number; allowed values 1, 2, 3, 4, 5; required"
        },
        "responses": {
          "200": {
            "description": "calculated BMR an avatar can also be displayed if there is one",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "client": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "email": {
                          "type": "string"
                        },
                        "_id": {
                          "type": "string"
                        },
                        "_v": {
                          "type": "number"
                        },
                        "height": {
                          "type": "number"
                        },
                        "currentWeight": {
                          "type": "number"
                        },
                        "desiredWeight": {
                          "type": "number"
                        },
                        "birthday": {
                          "type": "string",
                          "format": "date",
                          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                        },
                        "blood": {
                          "type": "number"
                        },
                        "sex": {
                          "type": "string"
                        },
                        "levelActivity": {
                          "type": "number"
                        },
                        "BMR": {
                          "type": "number"
                        },
                        "timeForSport": {
                          "type": "number"
                        }
                      }
                    }
                  },
                  "example": {
                    "client": {
                      "_id": "6537dcf0ab10068d0d3e9897",
                      "email": "1223test@gmail.com",
                      "name": "mymykola",
                      "__v": 0,
                      "BMR": 2170,
                      "birthday": "2003-10-10T00:00:00.000Z",
                      "blood": 4,
                      "currentWeight": 55,
                      "desiredWeight": 55,
                      "height": 155,
                      "levelActivity": 4,
                      "sex": "male",
                      "timeForSport": 110
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
