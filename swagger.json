  "info": {
    "version": "1.0.0",
    "title": "Power plus",
    "description": "backend for react app Power plus"
  },
  "servers": [
    {
      "url": "https://teamproject-powerpulse-group4.onrender.com/"
    }
  ],
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "parameters": {
      "Authorization": {
        "name": "Authorization",
        "in": "header",
        "required": true,
        "schema": {
          "type": "string"
        },
        "description": "Bearer Token"
      }
    }
